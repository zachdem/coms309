<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimary"
    tools:context="com.user.MainActivity">


    <TextView
        android:id="@+id/order_notificiation_tv"
        android:layout_width="401dp"
        android:layout_height="23dp"
        android:layout_marginStart="6dp"
        android:layout_marginTop="319dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="389dp"
        android:gravity="center"
        android:text=""
        android:textColor="@color/colorPrimaryDark"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/hungry_text2"
        android:layout_width="146dp"
        android:layout_height="35dp"
        android:layout_marginStart="132dp"
        android:layout_marginTop="348dp"
        android:layout_marginEnd="133dp"
        android:layout_marginBottom="348dp"
        android:fontFamily="sans-serif-black"
        android:text=" Order History"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="@color/colorPrimaryDark"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/refresh_orders_button"
        android:layout_width="139dp"
        android:layout_height="49dp"
        android:layout_marginStart="136dp"
        android:layout_marginTop="609dp"
        android:layout_marginEnd="136dp"
        android:layout_marginBottom="73dp"
        android:backgroundTint="@color/colorPrimaryDark"
        android:fontFamily="sans-serif-black"
        android:text="@string/Refresh"
        android:textColor="@color/colorAccent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ListView
        android:id="@+id/order_list_view"
        android:layout_width="384dp"
        android:layout_height="201dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="398dp"
        android:layout_marginEnd="14dp"
        android:layout_marginBottom="132dp"
        android:focusableInTouchMode="false"
        android:isScrollContainer="false"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/cy_image"
        android:layout_width="75dp"
        android:layout_height="81dp"
        android:layout_marginStart="192dp"
        android:layout_marginTop="34dp"
        android:layout_marginEnd="144dp"
        android:layout_marginBottom="616dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/cyrun" />

    <TextView
        android:id="@+id/cyrun_homepage_text"
        android:layout_width="163dp"
        android:layout_height="60dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="55dp"
        android:layout_marginEnd="219dp"
        android:layout_marginBottom="616dp"
        android:fontFamily="sans-serif-black"
        android:text="@string/CyRUN"
        android:textAppearance="@style/TextAppearance.AppCompat.Display2"
        android:textColor="@color/colorPrimaryDark"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/order_button"
        android:layout_width="271dp"
        android:layout_height="78dp"
        android:layout_marginStart="70dp"
        android:layout_marginTop="223dp"
        android:layout_marginEnd="70dp"
        android:layout_marginBottom="430dp"
        android:backgroundTint="@color/colorPrimaryDark"
        android:fontFamily="sans-serif-black"
        android:text="Place an Order"
        android:textColor="@color/colorAccent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/hungry_text"
        android:layout_width="143dp"
        android:layout_height="47dp"
        android:layout_marginStart="134dp"
        android:layout_marginTop="164dp"
        android:layout_marginEnd="134dp"
        android:layout_marginBottom="520dp"
        android:fontFamily="sans-serif-black"
        android:text="Hungry?"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="@color/colorPrimaryDark"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>